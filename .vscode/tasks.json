{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "frontend-dev",
      "type": "npm",
      "script": "dev",
      "isBackground": true,
      "problemMatcher": {
        "base": "$tsc-watch",
        "background": {
          "activeOnStart": true,
          "beginsPattern": "ready in",
          "endsPattern": "press h to show help"
        }
      },
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "options": {
        "cwd": "frontend"
      }
    }
  ]
}